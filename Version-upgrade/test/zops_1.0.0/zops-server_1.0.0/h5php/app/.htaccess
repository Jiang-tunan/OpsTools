<IfModule mod_authz_core.c>
	# Apache 2.4
	Require all denied
</IfModule>

<IfModule !mod_authz_core.c>
	# Apache 2.2
	Order deny,allow
	Deny from all
</IfModule>

<IfModule mod_rewrite.c>

 RewriteEngine on

 RewriteBase /

 RewriteCond %{REQUEST_FILENAME} !-d

 RewriteCond %{REQUEST_FILENAME} !-f

 RewriteRule ^(.*)$ zabbix.php?s=/$1 [QSA,PT,L]

</IfModule>
